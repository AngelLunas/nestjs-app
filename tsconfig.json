{
  "compilerOptions": {
    "resolveJsonModule": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "*": ["*"]
    }
  },
  "include": [
    "./src/**/*"
, "test/unit/app.controller.spec.ts", "test/unit/coda/coda.service.spec.ts", "test/unit/com/mtronic/fahs/controller/fahs.controller.spec.ts", "test/unit/com/mtronic/fahs/service/actor.service.spec.ts"  ]
}
